cyfra = 0

while cyfra != 10:
    print(cyfra)
    cyfra += 1
    
print("Hello word!") #dodalam komentarz 



def wyznacz_kwadrat(liczba):
    #####
    # Funkcja pobiera liczbe i oblicza jej kwadrat
    #####
    return liczba**2

print("Wpisz cyfre")
liczba = int(input())
obliczony_kwadrat = wyznacz_kwadrat(liczba)

print(obliczony_kwadrat)
